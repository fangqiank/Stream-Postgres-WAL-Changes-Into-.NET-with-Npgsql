{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "Secret": "",
    "Issuer": "postgres-cdc-demo",
    "Audience": "cdc-dashboard-users"
  },
  "Database": {
    "MaxRetryCount": 5,
    "MaxRetryDelay": 30,
    "CommandTimeout": 30
  },
  "WebSocket": {
    "MaxMessageSize": 65536,
    "KeepAliveInterval": 30
  },
  "Replication": {
    "SlotName": "order_events_slot",
    "PublicationName": "cdc_publication",
    "HeartbeatInterval": 30
  }
}